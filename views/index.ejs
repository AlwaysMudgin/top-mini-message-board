<html>
  <head>
    <title>Mini Message Board</title>
  </head>
  <body>
    <%- include('navbar') %>
    <h1>Mini Message Board</h1>
    <main>
      <% messages.forEach(message => { %>
      <h3><%= message.user %></h3>
      <p><%= message.text %></p>
      <p><%= message.added %></p>
      <a href="/message/<%= message.id %>">Link</a>
      <% }) %>
    </main>
  </body>
</html>
